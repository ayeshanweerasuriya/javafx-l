<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SampleController">
   <children>
      <Button layoutX="151.0" layoutY="301.0" mnemonicParsing="false" onAction="#btnSignInOnAction" prefHeight="31.0" prefWidth="145.0" style="-fx-background-color: #eb4034;" text="SignIn" textFill="WHITE">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Label fx:id="lblTitle" alignment="CENTER" contentDisplay="CENTER" layoutX="64.0" layoutY="57.0" text="Hello World" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS" underline="true">
         <font>
            <Font name="Agency FB Bold" size="29.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" layoutX="149.0" layoutY="206.0" promptText="User Name" />
      <TextField fx:id="txtPassword" layoutX="149.0" layoutY="244.0" promptText="Password" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="246.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/shit.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
